{
    "type": "application",
    "description": "对比视频相似度的php工具",
    "keywords": [
        "php",
        "curseforge"
    ],
    "homepage": "https://github.com/snje1987/videocmp",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Yang Ming",
            "email": "yangming0116@163.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "ext-sqlite3": "*",
        "minifw/console": "^v1.0",
        "minifw/db": "^v1.0"
    },
    "autoload": {
        "psr-4": {
            "Org\\Snje\\Videocmp\\": "src/"
        }
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.5"
    },
    "scripts": {
        "test": "./vendor-bin/phpunit/vendor/bin/phpunit",
        "build": "./vendor-bin/box/vendor/bin/box compile"
    },
    "minimum-stability": "dev",
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true
        }
    },
    "extra": {
        "bamarni-bin": {
            "bin-links": false,
            "forward-command": true
        }
    }
}
